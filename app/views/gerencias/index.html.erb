<p id="notice"><%= notice %></p>

<h1>Gerencias</h1>

<table>
  <thead>
    <tr>
      <th>Sigla</th>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gerencias.each do |gerencia| %>
      <tr>
        <td><%= gerencia.sigla %></td>
        <td><%= gerencia.descricao %></td>
        <td><%= link_to 'Show', gerencia %></td>
        <td><%= link_to 'Edit', edit_gerencia_path(gerencia) %></td>
        <td><%= link_to 'Destroy', gerencia, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gerencia', new_gerencia_path %>
