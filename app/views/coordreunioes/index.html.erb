<h1>Coordenadores</h1>

<table>
  <thead>
    <tr>
      <th>Matricula</th>
      <th>Nome</th>
      <th>Funcao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coordreunioes.each do |coordreuniao| %>
      <tr>
        <td><%= coordreuniao.matricula %></td>
        <td><%= coordreuniao.nome %></td>
        <td><%= coordreuniao.funcao %></td>
        <td><%= link_to 'Visualizar', coordreuniao, :class => "glyphicon glyphicon-file" %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Coordenador', new_coordreuniao_path, :class => "btn btn-default" %><br><br>

