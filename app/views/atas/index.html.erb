<p id="notice"><%= notice %></p>

<h1>Atas</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Descricao</th>
      <th>Autor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @atas.each do |ata| %>
      <tr>
        <td><%= ata.codigo %></td>
        <td><%= ata.descricao %></td>
        <td><%= ata.autor %></td>
        <td><%= link_to 'Show', ata %></td>
        <td><%= link_to 'Edit', edit_ata_path(ata) %></td>
        <td><%= link_to 'Destroy', ata, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ata', new_ata_path %>
